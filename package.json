{
	"name": "@fohoov/zod-openapi-client",
	"version": "1.1.1",
	"type": "module",
	"description": "command line tools for creating ts with zod clients from openapi sepc files",
	"author": "@FoHoOV",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FoHoOV/zod-openapi-client",
		"directory": "packages/cli"
	},
	"contributors": [
		"@FoHoOV"
	],
	"keywords": [
		"ts",
		"typescript",
		"openapi",
		"client generator"
	],
	"scripts": {
		"build": "tsc --build && vite build",
		"dev-axios": "vite build && node ./dist/cli.js --client axios --uri https://project-management-fohoov-backend.vercel.app/openapi.json --out ./.tmp/axios",
		"dev-fetch": "vite build && node ./dist/cli.js --client fetch --uri https://project-management-fohoov-backend.vercel.app/openapi.json --out ./.tmp/fetch",
		"lint": "eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"import": "./dist/cli.js",
			"types": "./dist/cli.d.ts"
		}
	},
	"files": [
		"dist/",
		"README.md",
		"package.json",
		"CHANGELOG.md"
	],
	"bin": {
		"client-generator": "./dist/cli.js"
	},
	"dependencies": {
		"@apidevtools/swagger-parser": "^10.1.0",
		"@openapitools/openapi-generator-cli": "^2.15.0",
		"commander": "^12.1.0",
		"handlebars": "^4.7.8",
		"openapi-zod-client": "^1.18.3",
		"openapi3-ts": "^4.4.0",
		"ts-to-zod": "^3.13.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.13.1",
		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.1.1",
		"eslint": "^9.12.0",
		"eslint-plugin-n": "^17.11.1",
		"prettier": "^3.5.3",
		"typescript": "~5.6.3",
		"typescript-eslint": "^8.8.1",
		"vite": "^6.1.0",
		"vite-plugin-dts": "^4.5.0",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"peerDependencies": {
		"@openapitools/openapi-generator-cli": ">=2.15.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=22.13.1"
	},
	"packageManager": "pnpm@10.2.1+sha512.398035c7bd696d0ba0b10a688ed558285329d27ea994804a52bad9167d8e3a72bcb993f9699585d3ca25779ac64949ef422757a6c31102c12ab932e5cbe5cc92",
	"pnpm": {
		"onlyBuiltDependencies": [
			"@nestjs/core",
			"@openapitools/openapi-generator-cli",
			"esbuild"
		]
	}
}
