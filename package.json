{
	"name": "@fohoov/ts-client-generator",
	"version": "0.0.1",
	"type": "module",
	"description": "command line tools for creating ts clients from openapi sepc files",
	"author": "@FoHoOV",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FoHoOV/ts-client-generator",
		"directory": "packages/cli"
	},
	"contributors": [
		"@FoHoOV"
	],
	"keywords": [
		"ts",
		"typescript",
		"openapi",
		"client generator"
	],
	"scripts": {
		"build": "tsc --build && vite build",
		"dev-axios": "vite build && node ./dist/cli.js generate-clients --client axios --uri https://project-management-fohoov-backend.vercel.app/openapi.json --out ./.tmp/axios",
		"dev-fetch": "vite build && node ./dist/cli.js generate-clients --client fetch --uri https://project-management-fohoov-backend.vercel.app/openapi.json --out ./.tmp/fetch",
		"lint": "eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"import": "./dist/cli.js",
			"types": "./dist/cli.d.ts"
		}
	},
	"files": [
		"dist/",
		"README.md",
		"package.json",
		"CHANGELOG.md"
	],
	"bin": {
		"client-generator": "./dist/cli.js"
	},
	"dependencies": {
		"@apidevtools/swagger-parser": "^10.1.0",
		"@openapitools/openapi-generator-cli": "^2.15.0",
		"commander": "^12.1.0",
		"openapi3-ts": "^4.4.0",
		"ts-to-zod": "^3.13.0",
		"typed-openapi": "^0.9.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@changesets/cli": "^2.28.1",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.13.1",
		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.1.1",
		"eslint": "^9.12.0",
		"eslint-plugin-n": "^17.11.1",
		"typescript": "~5.6.3",
		"typescript-eslint": "^8.8.1",
		"vite": "^6.1.0",
		"vite-plugin-dts": "^4.5.0",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"publishConfig": {
		"access": "public"
	}
}
